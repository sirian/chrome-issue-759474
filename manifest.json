{
  "name": "Test",
  "manifest_version": 2,
  "version": "0.1",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
      "<all_urls>",
      "webRequest"
  ]
}